server:
  port: 3333


  cloud:
    gateway:
      mvc:
      routes:
      - id: welcome-api
        uri: lb://WELCOME-API
        predicates:
        - Path=/welcome
      - id: greet-api
        uri: lb://GREET-API
        predicates:
        - Path=/greet
spring:       
  application:
    name: CLOUD-API-GATEWAY
